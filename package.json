{
  "name": "@techspokes/typescript-wsdl-client",
  "version": "0.6.1",
  "description": "TypeScript WSDL â†’ SOAP client generator with full xs:attribute support, complex types, sequences, inheritance, and namespace-collision merging.",
  "keywords": [
    "wsdl",
    "soap",
    "typescript",
    "codegen",
    "generator",
    "xml",
    "xsd"
  ],
  "homepage": "https://github.com/techspokes/typescript-wsdl-client#readme",
  "bugs": {
    "url": "https://github.com/techspokes/typescript-wsdl-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techspokes/typescript-wsdl-client.git"
  },
  "license": "MIT",
  "author": "TechSpokes <contact@techspokes.com> (https://www.techspokes.com)",
  "contributors": [
    "Serge Liatko <contact@techspokes.com> (https://github.com/sergeliatko)"
  ],
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "wsdl-tsc": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dev": "tsx src/cli.ts",
    "watch": "tsx watch src/cli.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "exit 0",
    "smoke": "tsx src/cli.ts --help",
    "smoke:gen": "rimraf tmp && tsx src/cli.ts --wsdl examples/minimal/weather.wsdl --out tmp && tsc -p tsconfig.smoke.json",
    "ci": "npm run build && npm run typecheck && npm run smoke && npm run smoke:gen"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/yargs": "^17.0.33",
    "rimraf": "^6.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.5",
    "soap": "^1.3.0",
    "yargs": "^18.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/techspokes"
  }
}
